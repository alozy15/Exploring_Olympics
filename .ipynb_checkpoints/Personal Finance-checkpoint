  {
   "cell_type": "code",
   "execution_count": 111,
   "metadata": {},
   "outputs": [],
   "source": [
    "# now lets create a definition for easy value substitions\n",
    "\n",
    "def loan_info(loan, rate, term):\n",
    "    payment = np.round(-np.pmt(rate / 12, term, loan),2)\n",
    "    balance = loan\n",
    "    \n",
    "    index = range(0, term + 1)\n",
    "    columns = ['month', 'payment', 'interest', 'principle', 'balance']\n",
    "    amort_df = pd.DataFrame(index = index, columns = columns)\n",
    "    \n",
    "    amort_df.iloc[0]['month'] = 0\n",
    "    amort_df.iloc[0]['balance'] = balance\n",
    "    \n",
    "    for i in range(1, term + 1):\n",
    "        interest = round(rate / 12 * balance, 2)\n",
    "        principle = payment - interest\n",
    "        balance = balance - principle\n",
    "        \n",
    "        amort_df.iloc[i]['month']= i\n",
    "        amort_df.iloc[i]['payment']= payment\n",
    "        amort_df.iloc[i]['interest']= interest\n",
    "        amort_df.iloc[i]['principle']= principle\n",
    "        amort_df.iloc[i]['balance']= balance\n",
    "        \n",
    "    return amort_df\n",
    "\n",
    "    \n",
    "    "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 142,
   "metadata": {},
   "outputs": [],
   "source": [
    "# lets build amort schedule for a random home purchased for $647,000 @ 3.65 and 360 months\n",
    "loan = 647000 * (1-.0375)\n",
    "rate = .0365\n",
    "term = 360\n",
    "\n",
    "amort_df = loan_info(loan, rate, term)"
   ]
  },
