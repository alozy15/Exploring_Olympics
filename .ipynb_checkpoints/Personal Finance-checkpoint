    "# now lets create a bi monthly pay schedule for the middle and end of month\n",
    "sm = pd.date_range(start = today, end = end, freq = 'SM')\n",
    "print('SM:', sm)\n",
    "\n",
    "# month start \n",
    "ms = pd.date_range(start = today, end = end, freq = 'MS')\n",
    "\n",
    "print('\\n')\n",
    "print('MS:', ms)\n",
    "\n",
    "# lets add our income from working our ass off...\n",
    "income = pd.DataFrame(data = {'income': 4000},\n",
    "                     index = pd.date_range(start = today, end = end, freq = 'SM'))\n",
    "mortgage = pd.DataFrame(data = {'mortgage': 3800},\n",
    "                       index = pd.date_range(start = today, end = end, freq = 'MS'))\n",
