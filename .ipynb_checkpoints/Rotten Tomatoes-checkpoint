    "import glob"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_list= []\n",
    "\n",
    "for ebert_review in glob.glob('ebert_reviews/*txt'):\n",
    "    with open (ebert_review, encoding = 'utf-8') as file:\n",
    "        title = file.readline()[:-1]        \n",
    "        review_url = file.readline()[:-1]        \n",
    "        review_text = file.read()\n",
    "        \n",
    "        df_list.append({'title': title, 'review_url': review_url, 'review_text':review_text})\n",
    "\n",
    "df_ebert = pd.DataFrame(df_list, columns = ['title', 'review_url', 'review_text'])            "
   ]
  },
